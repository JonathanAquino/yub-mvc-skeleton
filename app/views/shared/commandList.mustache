<br />
{{#commands}}
  <span style="font-size:16px;font-weight:bold">{{name}}</span>
  {{#goldenEggDate}}
    {{#showGoldenEggLabels}}{{!jontodo: check}}
        <span style="color:red">(<a class="hint" style="color:red" href="/parser/parse?command=ge">Golden Egg</a>)</span>
    {{/showGoldenEggLabels}}
  {{/goldenEggDate}}
  <br />
  {{getDescriptionExcerpt}}<br />
  <span class="hint">
  {{uses}} uses
  -
  Created {{creationDate}}
  -
  Last used
  {{#lastUseDate}}{{lastUseDate}}{{/lastUseDate}}
  {{^lastUseDate}}{{creationDate}}{{/lastUseDate}}
  -
  </span>
  {{^goldenEggDate}}{{!jontodo: check}}
    <span class="nominate hint"><a class="hint" href="mailto:jonathan.aquino@gmail.com?subject=Nomination for a YubNub Golden Egg: {{getRawurlencodedName}}&body=Hey Jon, there's a great command that I want to see on the Golden Egg list: {{getRawurlencodedName}}">
    Nominate</a> - </span>
  {{/goldenEggDate}}
  <a href="/kernel/man?args={{getRawurlencodedName}}" class="description hint">Description</a>
  -
  <span class="implementation hint">{{getDisplayUrl}}</span><br />
  <br />
{{/commands}}
{{#pagination}}{{!jontodo: check}}
  <div style="margin-top:30px">
  Pages:
  <span style="font-size:22px">
  {{#previous}}
    <a href="?page={{.}}"><b>Previous</b></a>
  {{/previous}}
  {{#pages}}
    {{#ellipsis}}
      â€¦
    {{/ellipsis}}
    {{^ellipsis}}
      {{#current}}
        {{page}}
      {{/current}}
      {{^current}}
        <a href="?page={{page}}">{{page}}</a>
      {{/current}}
    {{/ellipsis}}
  {{/pages}}
  {{#next}}
    <a href="?page={{.}}"><b>Next</b></a>
  {{/next}}
  </span>
{{/pagination}}
</div>
